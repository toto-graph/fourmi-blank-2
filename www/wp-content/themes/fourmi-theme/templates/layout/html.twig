{% block html_head_container %}
{% include 'layout/html-header.twig' %}
	{% block head %}{% endblock %}
	</head>
{% endblock %}

	<body class="{{ body_class }}">
		{% block header %}
			{% include 'layout/header.twig' %}
		{% endblock %}
		
		{% block aside %}
			{% include 'layout/aside.twig' %}
		{% endblock %}

		<main class="main-page" role="main">
			{% block menu_home %}
			{% endblock %}

			{% block page_title %}
			{% endblock %}

			{% block breadcrumb %}
				{% include 'components/breadcrumb.twig' %}
			{% endblock %}

			{% block page_content %}
			{% endblock %}
		</main>
		
		{% block footer %}
			{% include 'layout/footer.twig' %}
		{% endblock %}

		{% block newsletter %}
			{% include 'components/newsletter.twig' %}
		{% endblock %}
		
		{{ function('wp_footer') }}

		<!-- Global site tag (gtag.js) - Google Analytics -->
		<script async src="https://www.googletagmanager.com/gtag/js?id=UA-155688776-1"></script>
		<script>
			window.dataLayer = window.dataLayer || [];
		  	function gtag(){dataLayer.push(arguments);}
  			gtag('js', new Date());
			gtag('config', '{{ options.google_analytics }}');
		</script>
	</body>
</html>


